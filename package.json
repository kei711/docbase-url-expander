{
  "name": "docbase-url-expander",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "esbuild-node-tsc",
    "esbuild": "node --require esbuild-register --",
    "lint": "run-p -l lint:*",
    "lint:es": "eslint --fix ./src",
    "lint:prettier": "prettier --write .",
    "lint:ts": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@slack/bolt": "3.12.0",
    "axios": "0.26.1",
    "dayjs": "1.11.1",
    "lru_map": "0.4.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.3",
    "@types/node": "17.0.18",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "esbuild": "0.15.7",
    "esbuild-node-tsc": "1.8.9",
    "esbuild-register": "3.3.3",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "7.0.4",
    "jest": "28.1.3",
    "lint-staged": "12.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "prettier-plugin-package": "1.3.0",
    "typescript": "4.8.3"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  }
}
