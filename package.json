{
  "name": "docbase-url-expander",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "esbuild-node-tsc",
    "esbuild": "node --require esbuild-register --",
    "lint": "run-p -l lint:*",
    "lint:es": "eslint --fix ./src",
    "lint:prettier": "prettier --write .",
    "lint:ts": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@slack/bolt": "3.9.0",
    "axios": "0.26.0",
    "dayjs": "1.10.7",
    "lru_map": "0.4.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.2",
    "@types/node": "17.0.18",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "esbuild": "0.14.23",
    "esbuild-node-tsc": "1.8.3",
    "esbuild-register": "3.3.2",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "prettier-plugin-package": "1.3.0",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  }
}
