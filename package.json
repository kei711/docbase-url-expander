{
  "name": "docbase-url-expander",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "esbuild-node-tsc",
    "esbuild": "node --require esbuild-register --",
    "lint": "run-p -l lint:*",
    "lint:es": "eslint --fix ./src",
    "lint:prettier": "prettier --write .",
    "lint:ts": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@slack/bolt": "3.12.0",
    "axios": "0.27.2",
    "dayjs": "1.11.6",
    "lru_map": "0.4.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.4",
    "@types/node": "17.0.45",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "esbuild": "0.19.2",
    "esbuild-node-tsc": "2.0.5",
    "esbuild-register": "3.4.2",
    "eslint": "8.47.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "28.1.3",
    "lint-staged": "12.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "2.3.4",
    "prettier-plugin-package": "1.3.1",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  }
}
